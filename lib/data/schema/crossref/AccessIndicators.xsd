    </xsd:element>
    <xsd:element name="license_ref">
        <xsd:complexType>
            <xsd:simpleContent>
                <xsd:extension base="xsd:anyURI">
                    <xsd:attribute name="start_date" type="xsd:date" use="optional"/>
                </xsd:extension>
            </xsd:simpleContent>
        </xsd:complexType>
    </xsd:element>
